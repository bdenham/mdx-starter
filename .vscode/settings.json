{
  "tabnine.experimentalAutoImports": true,
  "editor.inlineSuggest.enabled": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "sync.syncExtensions": true,
  "sync.autoDownload": true,
  "sync.autoUpload": false,
  "sync.forceDownload": true,
  "sync.forceUpload": false,
  "sync.quietSync": false,
  "sync.gist": "Narcissistic Passion Minified",
  "git.autofetch": true,
  "window.zoomLevel": 1,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.lineHeight": 1.1,
  "terminal.integrated.scrollback": 50000,
  "terminal.integrated.shellIntegration.history": 200,
  "terminal.integrated.tabs.defaultColor": "terminal.ansiYellow",
  "terminal.integrated.tabs.defaultIcon": "microscope",
  "explorer.confirmDelete": false,
  "terminal.integrated.cursorStyle": "line",
  "window.openFilesInNewWindow": "on",
  "window.openFoldersInNewWindow": "on",
  "prettier.bracketSameLine": true,
  "prettier.printWidth": 120,
  "prettier.prettierPath": "./node_modules/prettier",
  "prettier.configPath": ".prettierrc",
  "prettier.singleAttributePerLine": true,
  "print.alternateBrowser": false,
  "print.colourScheme": "Atom One"
}
